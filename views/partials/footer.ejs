<script src="/scripts/lib/jquery-3.5.1.min.js"></script>
<script src="/scripts/lib/bootstrap.min.js"></script>

<% if(typeof customScripts != 'undefined') { %>
    <% customScripts.forEach(customJs => { %>
        <%- '<script src="' + customJs + '"></script>' %>
    <% }) %>
<% } %>

<% if(visible) { %>
    <footer class="shadow mt-2 mb-5 py-3">
        <div class="container d-flex justify-content-around">
            <span class="green"><i class="text-dark far fa-copyright"></i> YelpCamp 2020</span>
            <a href="/about"><i class="text-dark fas fa-info"></i> About</a>
            <a id="github-anchor" href="https://github.com/ali-kilinc/yelp-camp" target="_blank"><i class="text-dark fab fa-github"></i> GitHub</a>
        </div>
    </footer>
<% } %>

</body>
</html>